            <!-- Inner content -->
            <div class="content-inner">
                <main role="main" class="pb-3">
                    @RenderBody()
                </main>
            </div>
            <!-- /inner content -->
        </div>
        <!-- /main content 
    </div>
    <!-- /page content -->

    @RenderSection("Scripts", required: false)
</body>
</html>
